@Article{neubert-etal-1995,
  Title                    = {{D}ispersal and pattern formation in a discrete-time predator-prey models},
  Author                   = {Neubert, M. G. and Kot, M and Lewis, M. A.},
  Journal                  = {Theoretical Population Biology},
  Year                     = {1995},
  Pages                    = {7-43},
  Volume                   = {48},

  Abstract                 = {We investigate the dispersal-driven instabilities that arise in a discrete-time predator-prey model formulated as a system of integrodifference equations. Integrodifference equations contain two components: (1) difference equations, which model growth and interactions during a sedentary stage, and (2) redistribution kernels, which characterize the distribution of dispersal distances that arise during a vagile stage. Redistribution kernels have been measured for a tremendous number of organisms. We derive a number of redistribution kernels from first principles. Integrodifference equations generate pattern under a far broader set of ecological conditions than do reaction-diffusion models. We delineate the necessary conditions for dispersal-driven instability for two-species systems and follow this with a detailed analysis of a particular predator-prey model. (C) 1995 Academic Press, Inc.},
  Bdsk-url-1               = {http://dx.doi.org/10.1006/tpbi.1995.1020},
  Doi                      = {10.1006/tpbi.1995.1020}
}

