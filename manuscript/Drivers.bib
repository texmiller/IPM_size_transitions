  keywords = {Beneficence, community, competition, Gradient, Interaction, Removal,
	Target, Transplant},
  language = {en},
  url = {http://onlinelibrary.wiley.com/doi/10.2307/3236049/abstract},
  urldate = {2016-09-21}
}

